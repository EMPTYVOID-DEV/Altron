    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vercel": "^35.2.1",
